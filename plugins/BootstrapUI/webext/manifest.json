{
	"manifest_version": 2,
	"name": "ZeroNet",
	"version": "0.12",
	"description": "ZeroNet access",
	"icons": {
		"48": "logo48.png",
		"96": "logo96.png"
	},
	"applications": {
		"gecko": {
			"id": "webext@zeronet.io"
		}
	},
	"background": {
		"scripts": [
			"proxy.js"
		]
	},
	"permissions": [
		"proxy",
		"webRequest",
		"webRequestBlocking",
		"tabs",
		"*://*.zeronet/*",
		"*://*/zeronet-bootstrap"
	],
	"browser_action": {
		"default_icon": {
			"48": "logo48.png",
			"96": "logo96.png"
		},
		"default_title": "ZeroNet"
	}
}